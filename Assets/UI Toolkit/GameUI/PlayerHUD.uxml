<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/UI%20Toolkit/GameUI/PlayerHUD.uss?fileID=7433441132597879392&amp;guid=89bbe73027f904b8da50bf55484c285f&amp;type=3#PlayerHUD"/>
    <ui:VisualElement name="weapon-info-container" picking-mode="Ignore">
        <ui:Label name="weapon-type-label" text="Weapon Name" enabled="true" style="display: none;"/>
        <ui:Label name="reloading-label" text="RELOADING"/>
        <ui:VisualElement name="weapon-icon" style="display: none;"/>
        <ui:VisualElement name="ammo-container">
